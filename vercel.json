{
  "version": 2,
  "name": "twitter-api-dashboard",
  "framework": null,
  "buildCommand": null,
  "outputDirectory": ".",
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "assets/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "public": true
} 